class Solution:
    def productExceptSelf(self, nums: List[int]) -> List[int]:
        nums_copy = []
        nums_copy = nums.copy()
        result_array = []
        for i in range(0, len(nums)):
            nums[i] = 1
            result = numpy.prod(nums)
            result_array.append(result)
            nums = nums_copy.copy()
        return result_array
